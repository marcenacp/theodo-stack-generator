php_opcache_enable: 0
dev_env: true
# List of databases to be created
postgresql_databases:
  - name:  template
    hstore: yes
    uuid_ossp: yes

# List of users to be created
postgresql_users:
  - name:  template
    pass:  template
postgresql_user_privileges:
  - name:  template
    db:  template
    priv: "ALL"
    role_attr_flags: "SUPERUSER"
